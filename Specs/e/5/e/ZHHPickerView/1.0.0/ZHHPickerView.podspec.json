{
  "name": "ZHHPickerView",
  "version": "1.0.0",
  "summary": "iOS 日期选择器、字符串选择器（单列/多列/联动）",
  "description": "基于 UIPickerView 的底部弹窗选择器。支持日期选择（多种模式：年月日、时分、年月日时分等）、\n字符串单列/多列/多级联动选择；支持 plist/json 数据源、自定义样式与多语言。",
  "homepage": "https://github.com/yue5yueliang/ZHHPickerView",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "桃色三岁": "136769890@qq.com"
  },
  "source": {
    "git": "https://github.com/yue5yueliang/ZHHPickerView.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": [
    "5.0"
  ],
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "ZHHPickerView/Classes/**/*.swift",
      "resources": "ZHHPickerView/Classes/DatePicker/ZHHPickerView.bundle",
      "frameworks": [
        "UIKit",
        "Foundation"
      ]
    },
    {
      "name": "Base",
      "source_files": "ZHHPickerView/Classes/Base/**/*.swift",
      "frameworks": [
        "UIKit",
        "Foundation"
      ]
    },
    {
      "name": "StringPicker",
      "source_files": "ZHHPickerView/Classes/StringPicker/**/*.swift",
      "frameworks": [
        "UIKit",
        "Foundation"
      ],
      "dependencies": {
        "ZHHPickerView/Base": [

        ]
      }
    },
    {
      "name": "DatePicker",
      "source_files": "ZHHPickerView/Classes/DatePicker/**/*.swift",
      "resources": "ZHHPickerView/Classes/DatePicker/ZHHPickerView.bundle",
      "frameworks": [
        "UIKit",
        "Foundation"
      ],
      "dependencies": {
        "ZHHPickerView/Base": [

        ]
      }
    }
  ],
  "swift_version": "5.0"
}
